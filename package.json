{
  "name": "minepack",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "compile": "bun build ./src/main.ts --compile --outfile ./build/minepack.exe",
    "compile:win-x64": "bun build src/main.ts --compile --target=bun-windows-x64 --outfile ./build/minepack-win-x64.exe",
    "compile:mac-x64": "bun build src/main.ts --compile --target=bun-darwin-x64 --outfile ./build/minepack-mac-x64",
    "compile:mac-arm64": "bun build src/main.ts --compile --target=bun-darwin-arm64 --outfile ./build/minepack-mac-arm64",
    "compile:linux-x64": "bun build src/main.ts --compile --target=bun-linux-x64 --outfile ./build/minepack-linux-x64",
    "compile:linux-arm64": "bun build src/main.ts --compile --target=bun-linux-arm64 --outfile ./build/minepack-linux-arm64",
    "compile:linux-x64-musl": "bun build src/main.ts --compile --target=bun-linux-x64-musl --outfile ./build/minepack-linux-x64-musl",
    "compile:linux-arm64-musl": "bun build src/main.ts --compile --target=bun-linux-arm64-musl --outfile ./build/minepack-linux-arm64-musl",
    "compile:all": "bun run compile:win-x64 && bun run compile:mac-x64 && bun run compile:mac-arm64 && bun run compile:linux-x64 && bun run compile:linux-arm64 && bun run compile:linux-x64-musl && bun run compile:linux-arm64-musl",
    "minepack": "bun run src/main.ts"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/xml2js": "^0.4.14",
    "chalk": "^5.4.1",
    "fs-extra": "^11.3.0",
    "fuse.js": "^7.1.0",
    "toml": "^3.0.0",
    "xml2js": "^0.6.2"
  },
  "version": "2"
}